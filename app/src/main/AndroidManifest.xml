<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.accioma.telecosfacturamovil" >

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

<!--     AIzaSyCi3lUnTv4qxpK4-rrCn3sJRMMmPQ5OPjc -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <activity
            android:name=".activity.InvoiceListActivity"
            android:label="@string/title_activity_invoice" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.ProductListActivity"
            android:label="@string/title_activity_product_list" >
        </activity>
        <activity
            android:name=".activity.CustomerListActivity"
            android:label="@string/title_activity_customer_list" >
        </activity>
        <activity
            android:name=".activity.InvoiceFormActivity"
            android:label="@string/title_activity_invoice_form" >
        </activity>
        <activity android:name=".activity.CustomerFormActivity"
           android:label="@string/title_activity_customer_form" >
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".activity.CustomerListActivity">
            </meta-data>
        </activity>
        <activity android:name=".activity.SettingsFormActivity"
            android:label="@string/action_settings" >
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".activity.CustomerListActivity">
            </meta-data>
        </activity>
    </application>

</manifest>
